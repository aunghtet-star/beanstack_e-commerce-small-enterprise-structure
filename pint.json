{
    "preset": "laravel",
    "rules": {
        "no_unused_imports": true,
        "concat_space": {
            "spacing": "one"
        },
        "function_declaration": {
            "closure_fn_spacing": "one"
        }
    },
    "exclude": [
        "vendor",
        "storage",
        "bootstrap/cache",
        "node_modules"
    ]
}